---
apiVersion: v1
kind: Event
metadata:
  name: restore-error.{{ now }}
  namespace: "{{ ansible_operator_meta.namespace }}"
involvedObject:
  apiVersion: eda.ansible.com/v1alpha1
  kind: {{ kind }}
  name: {{ ansible_operator_meta.name }}
  namespace: "{{ ansible_operator_meta.namespace }}"
message: {{ error_msg }}
reason: RestoreFailed
type: Warning
firstTimestamp: {{ now }}
lastTimestamp: {{ now }}
count: 1
